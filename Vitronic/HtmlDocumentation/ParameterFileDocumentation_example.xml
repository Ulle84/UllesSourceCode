<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<?xml-stylesheet type="text/xsl" href="ViParameterFileDocumentation.xsl"?>
<ViParameter>
  <ViAcquiGenICam Version="2">
    <WatchDog>
      <WatchInterval Type="integer" Value="1"/>
      <QuiteRestart Type="bool" Value="false"/>
      <ReportTemperatur Type="bool" Value="true"/>
      <ReportStatistics Type="bool" Value="false"/>
    </WatchDog>
    <Camera UseDevice="true">
      <DeviceName Value="ViAcquiGenICam0" Type="string" Description="Name des Geräts" Editor="FREETEXT" />
